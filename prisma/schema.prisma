generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:dev.sqlite"
}

model discounts {
  id           Int              @id @default(autoincrement())
  amount       Float
  customerTags String?
  productTags  String?
  status       String
  discountType String
  discountId   String
  endsAt       DateTime
  startsAt     DateTime
  title        String
}

model session {
  id          String    @id
  shop        String
  state       String
  isOnline    Boolean   @default(false)
  scope       String?
  expires     DateTime?
  accessToken String
  userId      BigInt?
}

model shopifyFunction {
  id       String @id
  title    String
  api_type String
}
